{
  "name": "nestjs-serverless-workflow",
  "version": "0.0.1",
  "description": "Workflow and State Machines for NestJS",
  "license": "MIT",
  "author": "Thomas Do (tung-dnt)",
  "scripts": {
    "build": "nest build --builder=webpack",
    "clean": "del-cli -f \"./dist\" && del-cli -f \"./*.tsbuildinfo\"",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "npm run clean",
    "start": "nest start",
    "test": "jest --config jest.config.cjs --no-watchman ",
    "test:cov": "jest --coverage --config jest.config.cjs --no-watchman",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch --config jest.config.cjs --no-watchman",
    "predeploy": "npm run build && node scripts/deps.js",
    "deploy": "serverless deploy --stage production",
    "remove": "serverless remove",
    "logs": "serverless logs -f orderworkflow"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.896.0",
    "@aws-sdk/client-sqs": "^3.893.0",
    "@aws-sdk/lib-dynamodb": "^3.896.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/testing": "^11.1.3",
    "@swc-node/register": "^1.6.8",
    "@swc/cli": "^0.1.63",
    "@swc/core": "1.3.102",
    "@types/aws-lambda": "8.10.152",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "del-cli": "^6.0.0",
    "dynamodb-toolbox": "^2.7.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "serverless": "^4.19.1",
    "serverless-lift": "^1.32.0",
    "serverless-plugin-include-dependencies": "^6.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.8.3",
    "uuidv7": "^1.0.2",
    "webpack": "^5.89.0"
  },
  "_moduleAliases": {
    "@/*": "*"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "keywords": [
    "nestjs",
    "workflow",
    "state-machine",
    "fsm",
    "finite-state-machine",
    "typescript",
    "event-driven",
    "process-automation",
    "business-process",
    "workflow-engine",
    "state-management",
    "transition",
    "nodejs",
    "serverless",
    "orchestration",
    "lambda"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/@nestjs-serverless-workflow.git"
  },
  "bugs": {
    "url": "https://github.com/@nestjs-serverless-workflow/issues"
  },
  "homepage": "https://github.com/@nestjs-serverless-workflow#readme",
  "overrides": {
    "serverless-lift": {
      "serverless": "$serverless"
    }
  }
}
